{
  "name": "七麦排名小扒机器人",
  "version": "1.0",
  "description": "Parse And Download Rank Info As CSV",
  "manifest_version": 2,
  "permissions": ["activeTab", "storage", "declarativeContent", "downloads"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/icon.png",
    "32": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  }
}
